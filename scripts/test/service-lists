# quick list of service status checks

sudo systemctl status unionfs
sudo systemctl status unionfs-encrypt
sudo systemctl status rclone
sudo systemctl status rclone-en
sudo systemctl status rclone-encrypt
sudo systemctl status plexdrive

# quick list of service restarts

sudo systemctl restart unionfs
sudo systemctl restart unionfs-encrypt
sudo systemctl restart rclone
sudo systemctl restart rclone-en
sudo systemctl restart rclone-encrypt
sudo systemctl restart plexdrive
